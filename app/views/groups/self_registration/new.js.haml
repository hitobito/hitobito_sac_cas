- if params[:autosubmit].present?
  - markup = j(render(SelfRegistration::FeeComponent.new(group: @group, birthdays: entry.birthdays), layout: false))
  $('#fees').replaceWith("#{markup}")
- else
  $('form[data-controller="autosubmit"]').replaceWith("#{j(render('form', entry: entry))}");
  $("[data-provide=entity]").each(App.setupEntityTypeahead);
